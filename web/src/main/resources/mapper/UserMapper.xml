<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.puhuilink.qbs.core.web.mapper.UserMapper">

    <select id="getByUsername" resultType="com.puhuilink.qbs.core.web.entity.User">
        select *
        from t_user
        where username = #{username}
        limit 1
    </select>
</mapper>
